pipeline {
  agent any
  stages {
    stage('build') {
      steps {
        sh 'npm install'
      }
    }
    stage('Delivery') {
      when {
        branch 'master'
      }
      docker.withRegistry('https://hub.docker.com', 'dockerhub') {
        def image = docker.build("vladobrdaric/express-calculator")
	image.push()
      }
      steps {
        sh 'echo "Do this ..."'
      }
    }
  }
}
