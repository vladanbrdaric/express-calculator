pipeline {
  agent any
  stages {
    stage('build') {
      steps {
        sh 'npm install'
        sh 'cat Jenkinsfile-multibranch-express-calculator'
      }
    }
    stage('unit-test') {
      when {
        branch 'origin/feature/*'
      }
      steps {
        sh 'npm run unit-test'
      }
    }
    
    stage('integration-test') {
      when {
        branch 'origin/develop'
      }
      steps {
        sh 'npm run integration-test'
      }
    }
  }
}
